{
  "openapi": "3.1.0",
  "info": {
    "title": "Substrate Usage",
    "version": "0.0.1",
    "contact": {
      "email": "support@substrate.run"
    },
    "description": "Models for tracking API usage"
  },
  "servers": [
    {
      "url": "https://api.substrate.run",
      "description": "The production Substrate API"
    }
  ],
  "components": {
    "schemas": {
      "BaseExecutionUsage": {
        "type": "object",
        "description": "Base model for execution usage.",
        "properties": {
          "execution_seconds": {
            "type": "float",
            "description": "Total execution time."
          },
          "machine_config": {
            "type": "string",
            "description": "Machine config string in GPU(H100, count=1) format."
          }
        },
        "required": [
          "execution_seconds",
          "machine_config"
        ]
      },
      "XTTSUsage": {
        "allOf": [
          {
            "$ref": "#/components/schemas/BaseExecutionUsage"
          },
          {
            "type": "object",
            "properties": {
              "input_audio_bytes": {
                "type": "integer",
                "description": "Size of speaker conditioning audio."
              },
              "input_audio_seconds": {
                "type": "number",
                "format": "float",
                "description": "Length of speaker conditioning audio."
              },
              "output_audio_bytes": {
                "type": "integer",
                "description": "Size of output audio"
              },
              "output_audio_seconds": {
                "type": "number",
                "format": "float",
                "description": "Length of output audio"
              }
            },
            "required": [
              "input_audio_bytes",
              "input_audio_seconds",
              "output_audio_bytes",
              "output_audio_seconds"
            ]
          }
        ]
      },
      "SegmentAnythingUsage": {
        "allOf": [
          {
            "$ref": "#/components/schemas/BaseExecutionUsage"
          },
          {
            "type": "object",
            "properties": {
              "input_image_bytes": {
                "type": "integer",
                "description": "Size of input image."
              },
              "input_image_width": {
                "type": "integer",
                "description": "Widht of input."
              },
              "input_image_height": {
                "type": "integer",
                "description": "Height of input."
              }
            },
            "required": [
              "input_image_bytes",
              "input_image_width",
              "input_image_height"
            ]
          }
        ]
      },
      "GroundingDINOUsage": {
